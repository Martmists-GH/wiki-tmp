<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Team EXR{% endblock %}</title>
        <meta charset="utf-8" />
        <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
        <meta name="viewport" content="width=device-width" />

        <!-- Nav color -->
        <meta name="theme-color" content="#000000" />
        <meta name="msapplication-navbutton-color" content="#000000" />
        <meta name="apple-mobile-web-app-status-bar-style" content="#000000" />

        <!-- OG tags -->
        <meta content="{{ request.url }}" property="og:url" />
        <meta content="{% block og_type %}website{% endblock %}" property="og:type" />
        <meta content="{% block og_title %}EXR.team{% endblock %}" property="og:title" />
        <meta content="{% block og_description %}Team EXR's Website{% endblock %}" property="og:description" />
        <meta content="{% block og_image %}{{ request.baseUrl }}/static/img/og/banner.png{% endblock %}" property="og:image" />

{#        <!-- Twitter tags -->#}
{#        <meta name="twitter:site" content="@TeamEXR" />#}
{#        <meta name="twitter:card" content="summary" />#}

        <!-- Favicon -->
        <link href="/static/img/favicon.ico" rel="icon">

        <!-- Styles -->
        <link href="/static/css/fontawesome-all.min.css" rel="stylesheet">
        <link href="/static/css/halfmoon.min.css" rel="stylesheet">
        <link href="/static/css/halfmoon-docs.min.css" rel="stylesheet">
        <link href="/static/css/halfmoon-highlight.min.css" rel="stylesheet">
        <link href="/static/css/style.css" rel="stylesheet">

        <!-- Scripts -->
        <script type="text/javascript" src="/static/js/halfmoon.min.js"></script>
        <script type="text/javascript" src="/static/js/highlight.min.js"></script>
        <script type="text/javascript" src="/static/js/index.js"></script>
        {% block head %}{% endblock %}
    </head>

    <body class="dark-mode with-custom-webkit-scrollbars with-custom-css-scrollbars" data-set-preferred-mode-onload="true">
        <div class="page-wrapper with-navbar with-transitions {% if sidebar %}with-sidebar" data-sidebar-type="overlayed-sm-and-down{% endif %}">
            {% include "components/navbar" with {"sidebar": sidebar} %}
            {% block page_content %}{% endblock %}
        </div>
        <script type="text/javascript">window.dispatchEvent(new CustomEvent('exr:page_loaded'));</script>
    </body>
</html>
